<?xml version="1.0" encoding="UTF-8"?>
<api context="/auth" name="AuthService" xmlns="http://ws.apache.org/ns/synapse">
    <resource methods="POST">
        <inSequence>
            <log>
                <property name="message" value="login invoked"/>
            </log>
            <send>
                <endpoint key="base_path"/>
            </send>
        </inSequence>
        <outSequence>
            <send/>
        </outSequence>
        <faultSequence/>
    </resource>
</api>
