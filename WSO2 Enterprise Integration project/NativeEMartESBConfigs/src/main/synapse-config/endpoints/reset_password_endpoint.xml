<?xml version="1.0" encoding="UTF-8"?>
<endpoint name="reset_password_endpoint" xmlns="http://ws.apache.org/ns/synapse">
    <address uri="http://localhost:5000/api/auth/user/reset-password">
        <suspendOnFailure>
            <initialDuration>-1</initialDuration>
            <progressionFactor>1.0</progressionFactor>
        </suspendOnFailure>
        <markForSuspension>
            <retriesBeforeSuspension>0</retriesBeforeSuspension>
        </markForSuspension>
    </address>
</endpoint>
